entropia <- function(a,b){
total <- a + b
return (a/total) * log2(a/total) - (b/total) * log2(b/total)
}
entropia(22,13)
entropia(19,2)
entropia(3,11)
entropia(19,2) + entropia(3,11) - entropia(22,13)
